.woocommerce-checkout #payment ul.payment_methods li { list-style: none; }

#add_payment_method #payment, .woocommerce-cart #payment, .woocommerce-checkout #payment { background: rgb(255, 255, 255); border: 1px solid rgb(238, 238, 238); padding: 30px; }

