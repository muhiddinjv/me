.woocommerce a.remove:hover { background: rgb(255, 164, 173); color: rgb(255, 255, 255); }

.woocommerce-error, .woocommerce-info, .woocommerce-message { background-color: rgb(255, 255, 255); border-top-color: rgb(255, 255, 255); border-bottom: 1px solid rgb(238, 238, 238); }

